{
    //you can define multiple editors
    propertyEditors: [
        {
            /*this must be a unique alias*/
            alias: "Custom.FontawesomePicker",
            /*the name*/
            name: "Fontawesome Picker",
            /*the icon*/
            icon: "icon-code",
            /*grouping for "Select editor" dialog*/
            group: "Pickers",
            /*the HTML file we will load for the editor*/
            editor: {
                view: "~/App_Plugins/FontAwesomePicker/fontawesomepicker.html",
				 valueType: "TEXT"
            }
        }
    ],
    //array of files we want to inject into the application on app_start
    javascript: [
        '~/App_Plugins/FontAwesomePicker/fontawesomepicker.controller.js'
    ],
	css:[
	'~/App_Plugins/FontAwesomePicker/css/font-awesome.min.css',
	'~/App_Plugins/FontAwesomePicker/css/fontawesomepicker.css'
	]
}